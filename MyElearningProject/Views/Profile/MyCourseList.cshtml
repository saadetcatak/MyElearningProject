@model List<MyElearningProject.DAL.Entities.Process>

@{
    Layout = null;
    int count = 0;
}

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Öğrenci Otomasyonu</title>
    <!-- Bootstrap Core CSS -->
    <link href="~/Templates/material-lite-master/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- chartist CSS -->
    <link href="~/Templates/material-lite-master/assets/plugins/chartist-js/dist/chartist.min.css" rel="stylesheet">
    <link href="~/Templates/material-lite-master/assets/plugins/chartist-js/dist/chartist-init.css" rel="stylesheet">
    <link href="~/Templates/material-lite-master/assets/plugins/chartist-plugin-tooltip-master/dist/chartist-plugin-tooltip.css" rel="stylesheet">
    <!--This page css - Morris CSS -->
    <link href="~/Templates/material-lite-master/assets/plugins/c3-master/c3.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="~/Templates/material-lite-master/css/style.css" rel="stylesheet">
    <!-- You can change the theme colors from here -->
    <link href="~/Templates/material-lite-master/css/colors/blue.css" id="theme" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<div class="card">
    <div class="card-block bg-info">
        <h4 class="text-white card-title">Kurs Listeniz</h4>
    </div>
    <div class="card-block">
        <div class="message-box contact-box">
            <h2 class="add-ct-btn"><button type="button" class="btn btn-circle btn-lg btn-success waves-effect waves-dark">←</button></h2>
            <div class="message-widget contact-widget">

                @foreach (var item in Model)
                {
                    count++;

                    <a href="#">
                        <div class="user-img"> <img src="~/Templates/material-lite-master/assets/images/users/1.jpg" alt="user" class="img-circle"> <span class="profile-status online pull-right"></span> </div>
                        <div class="mail-contnet">
                            <span class="mail-desc">@count-@item.Course.Title</span>
                        </div>
                    </a>
                }
            </div>
        </div>
    </div>
</div>
