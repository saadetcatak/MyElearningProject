@model List<MyElearningProject.DAL.Entities.WatchCourse>
@{
    ViewBag.Title = "AddVideo";
    Layout = "~/Views/Shared/_InstructorLayout.cshtml";
}

<h2>Kurs Videoları</h2>
@{
    int count = 0;
}
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Kurs Adı</th>
        <th>Video</th>
        <th>Video Ekle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@count</td>
            <td>@item.Course.Title</td>
            <td><iframe width="560" height="315" src=" @item.VideoUrl" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe></td>
            <td><a href="/InstructorCourse/AddVideo/">Video Ekle</a></td>
        </tr>
    }
</table>


