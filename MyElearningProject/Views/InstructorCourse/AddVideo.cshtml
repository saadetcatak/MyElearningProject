@model List<MyElearningProject.DAL.Entities.WatchCourse>
@{
    ViewBag.Title = "AddVideo";
    Layout = "~/Views/Shared/_InstructorLayout.cshtml";
    int count = 0;
}



<form method="post">
    <label>Yeni Video</label>
    <input type="hidden" name="CourseID" />
    <input type="hidden" name="VideoNumber" value="@count+1" />
    <input type="text" name="VideoUrl" class="form-control" />
    <br />
    <br />
    <button class="btn btn-dark">Ekle</button>
</form>
