@model MyElearningProject.DAL.Entities.Map
@{
    ViewBag.Title = "AddMap";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Yeni Lokasyon Bilgileri</h3>
<form method="post">
    <br />
    <label>Lokasyon URL</label>
    <input type="text" name="LocationURL" class="form-control" />
    <br />
    <br />
    <div class="form-check">
        <input class="form-check-input" type="checkbox" name="Status" value="true" id="status">
        <label class="form-check-label" for="status">Durum</label>
    </div>
    <br />
    <br />
    <button class="btn btn-primary">Lokasyonu Kaydet</button>
</form>